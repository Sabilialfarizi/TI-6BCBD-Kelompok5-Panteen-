<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/home"
    tools:context=".sukses">

    <ImageView
        android:id="@+id/bulet"
        android:layout_width="260dp"
        android:layout_height="228dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="161dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="396dp"
        android:drawablePadding="10dp"
        android:paddingLeft="10dp"
        android:src="@drawable/bulat"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ceklis"
        android:layout_width="110dp"
        android:layout_height="114dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="207dp"
        android:layout_marginEnd="142dp"
        android:layout_marginBottom="464dp"
        android:drawablePadding="10dp"
        android:paddingLeft="10dp"
        android:src="@drawable/ceklis"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/elipse1"
        android:layout_width="169dp"
        android:layout_height="77dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="366dp"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="342dp"
        android:drawablePadding="10dp"
        android:paddingLeft="10dp"
        android:src="@drawable/ellipse"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="428dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="307dp"
        android:fontFamily="@font/montserrat"
        android:text="Pendaftaran Berhasil"
        android:textAlignment="center"
        android:textColor="@color/garis"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="284dp"
        android:layout_height="31dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="466dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="288dp"
        android:fontFamily="@font/tahoma"
        android:text="Selamat Datang di Aplikasi Panteen"
        android:textAlignment="center"
        android:textColor="@color/abu"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="250dp"
        android:layout_height="23dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="490dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="272dp"
        android:fontFamily="@font/tahoma"
        android:text="Terima Kasih!"
        android:textAlignment="center"
        android:textColor="@color/abu"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/selesai"
        android:layout_width="200dp"
        android:layout_height="46dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="601dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="138dp"
        android:background="@drawable/button_round"
        android:drawablePadding="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Selesai"
        android:onClick="Selesai"
        android:textAllCaps="false"
        android:textColor="@color/home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
